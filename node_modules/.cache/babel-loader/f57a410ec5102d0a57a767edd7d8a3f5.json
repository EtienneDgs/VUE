 class=\"container\">\n\n\n    <div class=\"row\">\n\n      <div class=\"addbutton\">\n        <button @click=\"addCatTitle()\"> add CATEGORY </button>\n      </div>\n\n      <div class=\"addCat\" id=\"addCat\">\n        <form @submit=\"AddCategory\">\n          <input type=\"text\" v-model=\"title\">\n          <input type=\"submit\" value=\"submit\">\n        </form>\n      </div>\n\n      <div class=\"categories\">\n        <Categories v-bind:categories=\"categories\" />\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport Categories from '@/components/Categories.vue'\nconst axios = require('axios').default;\n\nexport default {\n  name: 'Home',\n  components: {\n    Categories\n  },\n  data() {\n    return {\n      categories: [\n        {\n          id: 0,\n          title: \"titre cat 1\",\n          content: []\n        },\n                {\n          id: 1,\n          title: \"titre cat 2\",\n          content: []\n        }\n      ]\n    }\n  },\n  methods: {\n    async AddCategory() {\n      const res = await axios.get('http://postit.wac.under-wolf.eu/notes');\n\n      console.log(res);\n\n      this.categories.push({\n        title: this.title,\n        notes: []\n      });\n    },\n    addCatTitle() {\n      var divAddCat = document.querySelector(\".addCat\");\n      if (divAddCat.style.display == \"none\") {\n        divAddCat.style.display = \"inline-block\";\n      } else {\n        divAddCat.style.display = \"none\";\n      }\n      \n    }\n\n  }\n}\n</script>\n\n<style scoped>\ndiv .container {\n  display: block;\n}\n\n.addbutton {\n  display: block;\n  break-after: always;\n}\n\n.addCat {\n  display: none;\n}\n\n\n</style>>\n\n"],"sourceRoot":"src/views"}]}